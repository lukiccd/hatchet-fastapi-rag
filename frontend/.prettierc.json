{
    "arrowParens": "always",
    "bracketSameLine": false,
    "bracketSpacing": true,
    "insertPragma": false,
    "objectWrap": "collapse",
    "printWidth": 80,
    "proseWrap": "preserve",
    "quoteProps": "as-needed",
    "requirePragma": false,
    "semi": true,
    "singleQuote": true,
    "tabWidth": 4,
    "trailingComma": "all",
    "useTabs": true
}
